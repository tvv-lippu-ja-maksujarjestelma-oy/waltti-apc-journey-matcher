{
  "name": "waltti-apc-journey-matcher",
  "version": "0.0.1",
  "description": "Match the APC messages from the vehicles with GTFS Realtime messages to augment the APC messages with GTFS trip metadata",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:gtfsrt && rm -rf dist && tsc",
    "build:gtfsrt": "pbjs --target json-module --wrap es6 --out ./src/gtfsRealtime.js ./gtfs-realtime.proto && pbjs --target static-module --wrap es6 ./gtfs-realtime.proto | pbts --out ./src/gtfsRealtime.d.ts - && prettier --write ./src/gtfsRealtime.{js,d.ts}",
    "check-and-build": "npm run ts:check && npm run prettier:check && npm run eslint && npm run test && npm run build",
    "eslint": "eslint . --ext .ts --max-warnings=0",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "start": "node dist/index.js",
    "test": "jest",
    "ts:check": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tvv-lippu-ja-maksujarjestelma-oy/waltti-apc-journey-matcher.git"
  },
  "keywords": [
    "apc",
    "pulsar"
  ],
  "author": "haphut <haphut@mistmap.com>",
  "license": "EUPL-1.2",
  "bugs": {
    "url": "https://github.com/tvv-lippu-ja-maksujarjestelma-oy/waltti-apc-journey-matcher/issues"
  },
  "homepage": "https://github.com/tvv-lippu-ja-maksujarjestelma-oy/waltti-apc-journey-matcher#readme",
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "pino": "^7.6.5",
    "protobufjs": "^6.11.2",
    "pulsar-client": "https://github.com/apache/pulsar-client-node/tarball/40ac35e7668ef0a3dc7aa62fc670838f5871b543"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "./dist"
    ]
  },
  "config": {
    "pulsarVersion": "2.9.2"
  }
}
